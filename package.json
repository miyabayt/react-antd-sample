    "@babel/preset-typescript": "^7.23.2",
    "@craco/craco": "^7.1.0",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/css": "^11.11.2",
    "@emotion/eslint-plugin": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@tanstack/react-query": "^4.33.0",
    "@types/crypto-js": "^4.1.1",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.197",
    "@types/nookies": "^2.0.3",
