{
  "name": "react-antd-sample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "craco start",
    "dev": "npm run start",
    "build": "craco build",
    "lint": "eslint --ext .ts,.tsx --ignore-path .gitignore './src'",
    "lint:fix": "npm run lint -- --fix",
    "format": "npm run lint:fix && prettier --write --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx,json}' './*.{js,json}'",
    "test": "craco test",
    "eject": "craco eject"
  },
  "proxy": "http://localhost:8080",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "npm run format"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@craco/craco": "^7.1.0",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/css": "^11.11.2",
    "@emotion/eslint-plugin": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@tanstack/react-query": "^4.33.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/crypto-js": "^4.1.1",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.5.8",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash": "^4.14.197",
    "@types/nookies": "^2.0.3",
    "@types/nprogress": "^0.2.0",
    "@types/react-content-loader": "^4.0.0",
    "@types/react-helmet": "^6.1.6",
    "@types/react-transition-group": "^4.4.6",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "antd": "^5.8.4",
    "axios": "^1.4.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.7.0",
    "buffer": "^6.0.3",
    "crypto-js": "^4.1.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "file-saver": "^2.0.5",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.6.3",
    "js-cookie": "^3.0.5",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "path-to-regex": "^1.3.8",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "react-transition-group": "^4.4.5",
