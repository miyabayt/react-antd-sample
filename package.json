    "@types/react-transition-group": "^4.4.6",
    "antd": "^5.8.4",
    "axios": "^1.4.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.7.0",
    "buffer": "^6.0.3",
    "crypto-js": "^4.1.1",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "path-to-regex": "^1.3.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "react-transition-group": "^4.4.5",
    "typescript": "^5.1.6",
