{
  "name": "react-antd-sample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "craco start",
    "dev": "npm run start",
    "build:dev": "dotenvx run -f .env.dev -- craco build",
    "build:stg": "dotenvx run -f .env.stg -- craco build",
    "build:prd": "dotenvx run -f .env.prd -- craco build",
    "lint": "biome lint .",
    "lint:fix": "biome lint --fix --unsafe .",
    "format": "biome check --write .",
    "format:unsafe": "biome check --write --unsafe .",
    "test:unit": "craco test",
    "test:e2e": "npm run build:mock-server && npx playwright test",
    "build:mock-server": "docker build -t sample-mock-server ./mock-server",
    "eject": "craco eject",
    "prepare": "npx lefthook install"
  },
  "proxy": "http://localhost:8080",
  "dependencies": {
    "@ant-design/icons": "^5.5.0",
    "@craco/craco": "^7.1.0",
    "@dotenvx/dotenvx": "^1.13.2",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@tanstack/react-query": "^5.52.0",
    "@types/crypto-js": "^4.1.1",
    "@types/file-saver": "^2.0.5",
    "@types/lodash": "^4.14.197",
    "@types/nprogress": "^0.2.0",
    "@types/react-content-loader": "^4.0.0",
    "@types/react-helmet": "^6.1.6",
    "@types/react-transition-group": "^4.4.12",
    "ag-grid-community": "^33.0.4",
    "ag-grid-react": "^33.0.4",
    "antd": "^5.24.0",
    "axios": "^1.7.9",
    "buffer": "^6.0.3",
    "crypto-js": "^4.1.1",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "path-to-regex": "^1.3.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-rnd": "^10.5.2",
    "react-router-dom": "^7.2.0",
    "react-scripts": "^5.0.1",
    "react-transition-group": "^4.4.5",
    "react-use": "^17.5.1",
    "use-sync-external-store": "^1.4.0",
    "web-vitals": "^4.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@biomejs/biome": "1.9.4",
    "@emotion/babel-plugin": "^11.11.0",
    "@playwright/test": "^1.46.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.8",
    "@types/node": "^22.5.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
