{
  "name": "react-antd-sample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "craco start",
    "dev": "npm run start",
    "build": "craco build",
    "lint": "eslint --ext .ts,.tsx --ignore-path .gitignore ./src",
    "lint:fix": "npm run lint -- --fix",
    "format": "npm run lint:fix && prettier --write --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx,json}'",
    "test": "craco test",
    "eject": "craco eject"
  },
  "proxy": "http://localhost:8080",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "npm run format"
  },
  "dependencies": {
